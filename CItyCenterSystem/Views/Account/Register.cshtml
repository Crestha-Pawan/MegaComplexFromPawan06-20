@model FiboUser.Src.ViewModel.AccountViewModel.RegisterViewModel
@{ Layout = null; }
<!DOCTYPE html>

<html lang="en">

 

<head>

    <!-- Required meta tags-->

    <meta charset="UTF-8">

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta name="description" content="au theme template">

    <meta name="author" content="Hau Nguyen">

    <meta name="keywords" content="au theme template">

 

    <!-- Title Page-->

    <title>Parichaya Awas Bikash Pvt. Ltd.</title>

 

    <!-- Fontfaces CSS-->

    <link href="~/CoolAdmin-master/css/font-face.css" rel="stylesheet" />

    <link href="~/CoolAdmin-master/vendor/font-awesome-4.7/css/font-awesome.css" rel="stylesheet" />

    <link href="~/CoolAdmin-master/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

 

    <!-- Bootstrap CSS-->

    <link href="~/CoolAdmin-master/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

 

    <!-- Vendor CSS-->

    <link href="~/CoolAdmin-master/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/wow/animate.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/slick/slick.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/select2/select2.min.css" rel="stylesheet" media="all">

    <link href="~/CoolAdmin-master/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

 

    <!-- Main CSS-->

    <link href="~/CoolAdmin-master/css/theme.css" rel="stylesheet" />
    <style>
        .form-label{
            color:black
        }
    </style>
 

</head>

 

<body class="animsition">

    <div class="page-wrapper">

        <div class="page-content--bge5">

            <div class="container">

                <div class="login-wrap">

                    <div class="login-content">

                        <div class="login-logo">

                            <p style="text-align: center; color:blue"><h3>Parichaya Awas Bikash Pvt. Ltd.</h3><br /><b>Birtamode-5,Jhapa</b></p>

                            @*<a href="#">

                               

                                <img src="~/Login_v18/citycentre.jpg" alt="City Center System">

                            </a>*@

                        </div>

                        <div class="login-form">

                            <form class="form-horizontal form-material" id="loginform" asp-action="Register" asp-controller="Account">

                                <div class="form-group">

                                    <label for="UserName" class="form-label">UserName</label>

                                   <input type="text" class="form-control" asp-for="UserName" placeholder="UserName" required>

                                    <div class="invalid-feedback">

                                        Please Enter Email

                                    </div>

                                </div>

                                <div class="form-group">

                                    <label for="Email" class="form-label">Email</label>

                                    <input type="email" class="form-control" asp-for="Email" placeholder="Enter Email Address" required>

                                    <div class="invalid-feedback">

                                        Please Enter Username

                                    </div>

                                </div>

                                <div class="form-group">

                                    <label for="password" class="form-label">Password</label>

                                    <input type="password" class="form-control" asp-for="Password" required placeholder="Password">

                                    <div  class="invalid-feedback">

                                        Please Enter Password

                                    </div>

                                </div>

                                <div class="form-group">

                                    <label for="ConfirmPassword" class="form-label">ConfirmPassword</label>

                                    <input type="password" class="form-control" asp-for="ConfirmPassword" placeholder="Enter password" required>

                                    <div class="invalid-feedback">

                                        Please Enter Password

                                    </div>

                                </div>

                                <div class="login-checkbox">

                                    <label>

                                        <input type="checkbox" name="aggree">Agree the terms and policy

                                    </label>

                                </div>

                                <button class="au-btn au-btn--block au-btn--blue m-b-20" type="submit">register</button>

                                <!--<div class="social-login-content">-->

                                    @*<div class="social-button">

                                        <button class="au-btn au-btn--block au-btn--blue m-b-20">register with facebook</button>

                                        <button class="au-btn au-btn--block au-btn--blue2">register with twitter</button>

                                    </div>*@

                                <!--</div>-->

                            </form>

                            @*<div class="register-link">

                                <p>

                                    Already have account?

                                    <a href="#">Sign In</a>

                                </p>

                           </div>*@

                        </div>

                    </div>

                </div>

            </div>

        </div>

 

    </div>

 

    <!-- Jquery JS-->

    <script src="~/CoolAdmin-master/vendor/jquery-3.2.1.min.js"></script>

    <!-- Bootstrap JS-->

    <script src="~/CoolAdmin-master/vendor/bootstrap-4.1/popper.min.js"></script>

    <script src="~/CoolAdmin-master/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/slick/slick.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/wow/wow.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/animsition/animsition.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/CoolAdmin-master/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/CoolAdmin-master/vendor/select2/select2.min.js"></script>
    <!-- Main JS-->

    <script src="~/CoolAdmin-master/js/main.js"></script>

 

</body>

 
  
</html>

<!-- end document-->